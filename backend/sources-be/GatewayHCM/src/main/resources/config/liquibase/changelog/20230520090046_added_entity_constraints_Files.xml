        <addForeignKeyConstraint baseColumnNames="subject_id"
                                 baseTableName="files"
                                 constraintName="fk_files__subject_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subject"/>

        <addForeignKeyConstraint baseColumnNames="gallery_id"
                                 baseTableName="files"
                                 constraintName="fk_files__gallery_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gallery"/>
    </changeSet>
</databaseChangeLog>
